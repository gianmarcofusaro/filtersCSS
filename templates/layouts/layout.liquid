    <head>
        <!-- Title block allowing child pages to override -->
        <title>{% block title %}startProject{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no" />

		<!-- smart app banner -->
        <meta name="apple-itunes-app" content="app-id=1059442774">
		<!-- smart app banner -->

        <meta name="description" content="">
        <meta name="keywords" content="">

        <link rel="shortcut icon" type="image/ico" href="/favicon.ico" />
        <!-- Styles -->
        {{ "vendor/modernizr.min.js" | asset_url | script_tag }}
        {{ "application.css" | asset_url | stylesheet_tag }}

				<link href='http://fonts.googleapis.com/css?family=Lato:300,400' rel='stylesheet' type='text/css'>


        <!-- Style block allowing child pages to inject their own styles -->
        {% block styles %}{% endblock %}

    </head>
    <body>
        <div class="wrapper">
        		{% include "header" %}
            <!-- Include -->
            {% block content %}{% endblock %}
            {% include "footer" %}
        </div>
        <!-- Script block allowing child pages to inject their own scripts -->
        {% include "script" %}
    </body>
</html>